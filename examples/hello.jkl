# declare the types
let Slice = @Slice.0;
let Nat = @Nat.0;

let U64 = Nat(64);
let Char = Nat(8);
let CString = Slice(Char);

# declare the external functions
let puts = extern "puts" Fn(CString) -> struct {};

let main = fn(argc: U64, argv: Slice(CString)): U64 -> {
    puts(@array_to_slice(Char, 14)("Hello, world!\n"));
    0
};